function KillEnter(e){var iKeyCode=window.event.keyCode;if(iKeyCode==13)return false;}//document.onkeydown=KillEnter;
//Get A Object By ObjectId
function $(obj){return document.getElementById?document.getElementById(obj):null;}
//Get A ObjectArray By ObjectTagName
function $N(obj){return document.getElementsByTagName?document.getElementsByTagName(obj):new Array();}
function $TN(obj){return document.getElementsByName?document.getElementsByName(obj):new Array();}

function Check(){
if($("uName").value==""){
    alert("姓名称呼不能为空！");
    document.FeedBack.uName.focus();
    return false;
  }  

if($("From").value==""){
    alert("您是哪里人？");
    document.FeedBack.From.focus();
    return false;
  }

if($("Body").value==""){
    alert("留言内容不能为空！");
    document.FeedBack.Body.focus();
    return false;
  }

if($("verifyCode").value==""){
    alert("验证码不能为空！");
    document.FeedBack.verifyCode.focus();
    return false;
  }
document.FeedBack.submit(); 
}
</script>
